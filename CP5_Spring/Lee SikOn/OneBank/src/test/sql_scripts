truncate TABLE CUSTOMER
--select 'Insert into CUSTOMER (CUSTID,CUSTNAME,DOB,PHONENO,EMAIL,ISACTIVE,ADDR1,ADDR2,NRIC,PASSCODE,DATEUPD) values (' ||
select 'Insert into CUSTOMER (CUSTNAME,DOB,PHONENO,EMAIL,ISACTIVE,ADDR1,ADDR2,NRIC,PASSCODE,DATEUPD) values (' ||
--       TO_CHAR(CUSTID) || ',' || 
       '''' || CUSTNAME || '''' || ',' || 
       '''' || TO_CHAR(DOB,'DD-MON-YYYY') || '''' || ',' || 
       '''' || PHONENO || '''' || ',' ||
       '''' || EMAIL || '''' || ',' ||
       ISACTIVE || ',' || 
       '''' || ADDR1 || '''' || ',' ||
       '''' || ADDR2 || '''' || ',' ||
       '''' || NRIC || '''' || ',' ||
       '''' || PASSCODE || '''' || ',' ||
       '''' || TO_CHAR(DATEUPD,'DD-MON-YYYY') || '''' || ');'
       from customer

